  #string

  s="I am a string."
  print(type(s))

  #Boo;ean

  yes = True
  Print(type(yes))

  no = False
  print(type(no))

  #list -- ordered and changeable

  alpha_list = ["a","b"."c"]   #list initialization
  print(type(alpha_list))
  print(type(alpha_list[0]))
  alpha_list.append("d")
  print(alpha_list)

  #Tuole -- ordered and unchangeable

  alpha_tuple = ("a","b","c")  #tuple initialization
   print(type(alpha_tuple))    #will say tuple

   try:
       alpha_tuple[2] = "d"   #attempt the follow line
   except TyoeError:
         print("We can't add element to the tuples!")#print this message
         print(alpha_tuple)            #will print tuple